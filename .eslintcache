              <BasicInformation\r\n                                state={props.state}\r\n                                handleChangeState={props.handleChangeState}\r\n                                brands={props.brands}\r\n                                handleSelectBrand={props.handleSelectBrand}\r\n                                isPublic={props.isPublic}\r\n                            />\r\n                        </Col>\r\n\r\n                        <Col xs='12' md='4'>\r\n                            <Preview states={props.state} type='laptop' />\r\n                        </Col>\r\n                    </Row>\r\n                </Tab>\r\n\r\n                <Tab eventKey='detail' title='Detail Information'>\r\n                    <Row>\r\n                        <Col xs='12' md='8'>\r\n                            <DetailInformation\r\n                                state={props.state}\r\n                                handleChangeState={props.handleChangeState}\r\n                            />\r\n                        </Col>\r\n\r\n                        <Col xs='12' md='8'>\r\n\r\n                        </Col>\r\n                    </Row>\r\n                </Tab>\r\n            </Tabs>\r\n        </Form>\r\n    );\r\n}\r\n\r\nfunction BasicInformation (props) {\r\n    const state = props.state;\r\n    const options = [];\r\n    props.brands && props.brands.forEach((brand, index) => {\r\n        brand.brandOf.forEach((item) => {\r\n            if (item === 'Laptop') {\r\n                options.push(\r\n                    <option key={index} value={brand.name}>{brand.name}</option>\r\n                );\r\n            }\r\n        });\r\n    });\r\n\r\n    return (\r\n        <div style={{\r\n            padding: '15px', border: '1px solid rgba(0,0,0,0.095)',\r\n            borderRadius: '0 5px 5px 5px', marginBottom: '20px'\r\n        }}>\r\n            <Form.Group>\r\n                <Form.Label>Laptop's name</Form.Label>\r\n                <Form.Control \r\n                    type='text' name='name'\r\n                    placeholder='Enter name...'\r\n                    value={state.name}\r\n                    onChange={(e) => {props.handleChangeState(e)}}\r\n                />\r\n            </Form.Group>\r\n\r\n            <Form.Group>\r\n                <Form.Label>Brand</Form.Label>\r\n                <Form.Control name='brand' as='select' custom onChange={(e)=>{props.handleSelectBrand(e)}} >\r\n                    {state.brand === '' ?\r\n                        <option selected>Please select brand of laptop</option> :\r\n                        <option>Please select brand of laptop</option>}\r\n                    {options}\r\n                </Form.Control>\r\n            </Form.Group>\r\n\r\n            <Form.Group>\r\n                <Form.Label>Price</Form.Label>\r\n                <Form.Control \r\n                    type='text' name='price'\r\n                    placeholder='Enter price...'\r\n                    value={state.price}\r\n                    onChange={(e) => {props.handleChangeState(e)}}\r\n                />\r\n            </Form.Group>\r\n\r\n            <Form.Group>\r\n                <Form.Label>Thumbnail</Form.Label>\r\n                <Form.Control\r\n                    type='text' name='image'\r\n                    placeholder='Enter url...'\r\n                    value={state.image}\r\n                    onChange={(e) => {props.handleChangeState(e)}}\r\n                />\r\n            </Form.Group>\r\n\r\n            <Form.Group>\r\n                <Form.Label>Mode</Form.Label>\r\n                <Form.Check\r\n                    type='checkbox' label='Public' checked={state.public}\r\n                    onChange={props.isPublic}\r\n                />\r\n            </Form.Group>\r\n\r\n            <Button variant='primary' type='submit' style={{float: 'right'}}>Submit</Button>\r\n            <div className='clear-fix'></div>\r\n        </div>\r\n    );\r\n}\r\n\r\nfunction DetailInformation (props) {\r\n    const state = props.state;\r\n    return (\r\n        <div style={{padding: '15px',marginBottom: '20px', borderRadius: '5px', border: '1px solid rgba(0,0,0,.098)'}}>\r\n        <Form.Group>\r\n            <Form.Control\r\n                type='text' name='screen' placeholder='Screen' value={state.screen}\r\n                onChange={(e)=>{props.handleChangeState(e)}}\r\n            />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n            <Form.Control\r\n                type='text' name='operatingSystem' placeholder='Operating system'\r\n                value={state.operatingSystem}\r\n                onChange={(e)=>{props.handleChangeState(e)}}\r\n            />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control \r\n            type='text' name='graphicCard' placeholder='Graphic Card'\r\n            value={state.graphicCard}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n        />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control \r\n            type='text' name='storage' placeholder='Storage'\r\n            value={state.storage}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control \r\n            type='text' name='processor' placeholder='Processor'\r\n            value={state.processor}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control \r\n            type='text' name='ram' placeholder='RAM'\r\n            value={state.ram}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control\r\n            type='text' name='panelPorts' placeholder='Panel Ports'\r\n            value={state.panelPorts}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control\r\n            type='text' name='design' placeholder='Design'\r\n            value={state.design}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control\r\n            type='text' name='dimensions' placeholder='Dimensions'\r\n            value={state.dimensions}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control\r\n            type='text' name='pin' placeholder='Pin'\r\n            value={state.pin}\r\n            onChange={(e)=>{props.handleChangeState(e)}}\r\n          />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control as='textarea' name='box' value={state.box} placeholder=\"what's in the box?\" onChange={(e) => {props.handleChangeState(e)}} />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Label>Detailed pictures of the product</Form.Label>\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control type='text' name='image1' placeholder='Enter link image...' value={state.image1} onChange={(e) => {props.handleChangeState(e)}} />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control type='text' name='image2' placeholder='Enter link image...' value={state.image2} onChange={(e) => {props.handleChangeState(e)}} />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <Form.Control type='text' name='image3' placeholder='Enter link image...' value={state.image3} onChange={(e) => {props.handleChangeState(e)}} />\r\n        </Form.Group>\r\n\r\n        <Form.Group>\r\n          <div className=\"pr-pictures\">\r\n            <img src={state.image1} alt=\"Not found image link\" />\r\n          </div>\r\n\r\n          <div className=\"pr-pictures\">\r\n            <img src={state.image2} alt=\"Not found image link\" />\r\n          </div>\r\n\r\n          <div className=\"pr-pictures\">\r\n            <img src={state.image3} alt=\"Not found image link\" />\r\n          </div>\r\n        </Form.Group>\r\n\r\n        <Button variant=\"primary\" type=\"submit\" style={{float: 'right'}}>Submit</Button>\r\n        <div className=\"clear-fix\"></div>\r\n      </div>\r\n    );\r\n}\r\n\r\nexport {Laptop};","C:\\Users\\PC\\Desktop\\ReactApp\\d2shop\\src\\Components\\CP\\SPComponents\\ProductsList.js",["412","413","414","415","416","417","418","419","420","421","422","423","424","425","426","427","428"],"import React, {useState, useEffect} from 'react';\r\nimport FireDb from '../../../firebase';\r\nimport useCurrency from '../../../Hooks/useCurrency';\r\nimport {\r\n    Row, Col, Card, DropdownButton, ButtonGroup, Dropdown, Badge, Modal, Button, Tab,\r\n    Nav,\r\n    Form\r\n} from 'react-bootstrap';\r\nimport {\r\n    StarFill, ThreeDots, Pencil, Trash, FileLock, Globe, PencilSquare, GraphUp,\r\n    EmojiSmile, InfoCircle\r\n} from 'react-bootstrap-icons';\r\nimport { CKEditor } from '@ckeditor/ckeditor5-react';\r\nimport ClassicEditor from '@ckeditor/ckeditor5-build-classic';\r\n\r\nfunction ProductsList (props) {\r\n    const [modalShow, setModalShow] = useState(false);\r\n    const [state, setState] = useState(null);\r\n    const [prState,setPrState] = useState(null);\r\n    const brands = [];\r\n    const [brand, setBrand] = useState(brands);\r\n    const [listOfAcsrType, setListOfAcsrType] = useState([]);\r\n\r\n    let primaryKey = props.type;\r\n    let type = props.type;\r\n    function getPrList (snapshot) {\r\n        const prList = [];\r\n        snapshot.forEach((childSnapshot)=>{\r\n            let key = childSnapshot.key;\r\n            let data = childSnapshot.val();\r\n            prList.push({...data, key: key});\r\n        });\r\n        setState(prList);\r\n    }\r\n\r\n    function getAcsrType (snapshot) {\r\n        const acsrType = [];\r\n        snapshot.forEach((item)=>{\r\n          let data = item.val();\r\n          let key = item.key;\r\n          acsrType.push({...data, key: key});\r\n        });\r\n        setListOfAcsrType(acsrType);\r\n      }\r\n\r\n    function getBrands (snapshot) {\r\n        const temporaryData = [];\r\n        snapshot.forEach((childSnapshot)=>{\r\n            let val = childSnapshot.val();\r\n            temporaryData.push({name: val.name, brandOf: val.brandOf});\r\n        });\r\n        setBrand(temporaryData);\r\n    }\r\n\r\n    function handleChangeState (e) {\r\n        setPrState({...prState, [e.target.name]: e.target.value});\r\n    }\r\n\r\n    function getContent (data) {\r\n        setPrState({...prState, content: data});\r\n    }\r\n\r\n    useEffect(()=>{\r\n        FireDb.ref().child(primaryKey).on('value',getPrList);\r\n        return () => {\r\n            FireDb.ref().child(primaryKey).off('value',getPrList);\r\n        }\r\n    });\r\n\r\n    useEffect(()=>{\r\n        FireDb.ref().child('brands').once('value',getBrands)\r\n    }, []);\r\n\r\n    useEffect(()=>{\r\n        FireDb.ref().child('accessorytype').once('value',getAcsrType);\r\n    },[]);\r\n\r\n    function removePr (key) {\r\n        FireDb.ref(primaryKey).child(key).remove();\r\n    }\r\n\r\n    function handleMode (key, isPublic) {\r\n        FireDb.ref(primaryKey).child(key).update({public: !isPublic});\r\n    }\r\n\r\n    let classImg = \"cp-card-sm-img-c \";\r\n    let classImg2 = '';\r\n    switch (primaryKey) {\r\n        case 'laptop': \r\n            classImg2 = 'card-lt';\r\n            break;\r\n    \r\n        default:\r\n            classImg2 = 'card-sm'\r\n            break;\r\n    }\r\n\r\n    switch (type) {\r\n        case 'laptop': \r\n            type = 'Laptop';\r\n            break;\r\n        case 'tablet':\r\n            type = 'Tablet'\r\n            break;\r\n        case 'tv':\r\n            type = 'TV'\r\n            break;\r\n        case 'accessories':\r\n            type = 'Accessories'\r\n            break;\r\n        case 'help':\r\n            type = 'Help'\r\n            break;\r\n        default:\r\n            type = 'Smartphone'\r\n            break;\r\n    }\r\n\r\n    function handleUpdate (data) {\r\n        setModalShow(true);\r\n        setPrState(data);\r\n    }\r\n\r\n    function handleUpdateSubmit (key) {\r\n        FireDb.ref(primaryKey).child(key).update(prState);\r\n        setModalShow(false);\r\n    }\r\n\r\n    function removeAllRating (key) {\r\n        FireDb.ref(`${primaryKey}/${key}`).child('rating').remove().then(alert('Complete!'));\r\n    }\r\n\r\n    const data = [];\r\n    let i = 0;\r\n    if (state !== null) {\r\n        state.forEach((item, index)=>{\r\n            i = i + 1;\r\n            data.push(\r\n              <Col key={index} xs='12' md=\"3\" style={{marginBottom: '15px'}}>\r\n                <Card className=\"cp-card-sm\">\r\n                    <Card.Header style={{backgroundColor: 'white', color: '#3399FF', cursor: 'pointer'}}>\r\n                    <span>#{i}</span>\r\n                    {/* <div style={{display: 'inline-block', float: 'right'}}>4.5 <StarFill style={{color: 'gold', marginBottom: '6px'}} /></div> */}\r\n                    </Card.Header>\r\n                    <div onClick={()=>{handleUpdate(item)}} className=\"cp-card-sm-img-p\">\r\n                    <Card.Img className={classImg + classImg2} style={{\r\n                        width: '60%',\r\n                        textAlign: 'center'\r\n                        }} variant=\"top\" src={item.image} />\r\n                    </div>\r\n                    <Card.Body onClick={()=>{handleUpdate(item)}}>\r\n                    <Card.Title className=\"cp-card-sm-title\" >{item.name}</Card.Title>\r\n                    <Card.Title className=\"cp-card-sm-title\" >{item.title}</Card.Title>\r\n                    <Card.Text style={{color: '#dd0000', fontWeight: 'bold'}}>\r\n                        {item.price !== undefined ? '$' + item.price : ''}\r\n                    </Card.Text>\r\n                    </Card.Body>\r\n                    <Card.Footer style={{background: 'white', borderTop: 'none', paddingTop: '0'}}>\r\n                    {\r\n                        item.public === true ?\r\n                        <Badge pill variant=\"success\">Public</Badge> :\r\n                        <Badge pill variant=\"danger\">Private</Badge>\r\n                    }\r\n                    \r\n                    <DropdownButton\r\n                        className=\"cp-dr-dr\"\r\n                        variant=\"light\"\r\n                        style={{float: 'right',':hover' : {color: 'blue'}}}\r\n                        drop=\"up\"\r\n                        as={ButtonGroup}\r\n                        title={<ThreeDots style={{fontSize: '26px'}} />}\r\n                        >\r\n                        {\r\n                        item.public === true ? \r\n                        <Dropdown.Item onClick={(e) => {handleMode(item.key,item.public)}}><FileLock /> Private</Dropdown.Item> :\r\n                        <Dropdown.Item onClick={(e) => {handleMode(item.key,item.public)}}><Globe /> Public</Dropdown.Item>\r\n                        }\r\n                        <Dropdown.Item onClick={()=>{handleUpdate(item)}}><Pencil /> Update</Dropdown.Item>\r\n                        <Dropdown.Divider />\r\n                        <Dropdown.Item onClick={(e) => {removePr(item.key)}}><Trash /> Delete</Dropdown.Item>\r\n                    </DropdownButton>\r\n                    \r\n                    <div className=\"clear-fix\"></div>\r\n                    </Card.Footer>\r\n                    \r\n                </Card>\r\n              </Col>\r\n            );\r\n        });\r\n    }\r\n\r\n    return (\r\n        <Row>\r\n            {data}\r\n            <DetailModal\r\n                show={modalShow} state={prState} onHide={() => setModalShow(false)}\r\n                brands={brand} handleChangeState={handleChangeState}\r\n                handleUpdateSubmit={handleUpdateSubmit}\r\n                type={type}\r\n                listOfAcsrType={listOfAcsrType}\r\n                removeAllRating={removeAllRating}\r\n                getContent={getContent}\r\n            />\r\n        </Row>\r\n    );\r\n}\r\n\r\nfunction DetailModal (props) {\r\n    return (\r\n        <Modal\r\n          {...props}\r\n          size='xl'\r\n          aria-labelledby=\"contained-modal-title-vcenter\"\r\n          centered\r\n        >\r\n          <Modal.Header closeButton>\r\n              <h5>Information</h5>\r\n          </Modal.Header>\r\n\r\n          <Modal.Body>\r\n            <Tab.Container id=\"left-tabs-example\" defaultActiveKey=\"first\">\r\n                <Row>\r\n                    <Col sm={3}>\r\n                    <Nav variant=\"pills\" className=\"flex-column\">\r\n                        <Nav.Item>\r\n                          <Nav.Link eventKey=\"first\"><PencilSquare /> Edit</Nav.Link>\r\n                        </Nav.Item>\r\n                        <Nav.Item>\r\n                          <Nav.Link eventKey=\"second\"><GraphUp /> Analytics</Nav.Link>\r\n                        </Nav.Item>\r\n                        <Nav.Item>\r\n                          <Nav.Link eventKey=\"third\"><EmojiSmile /> Review & Rate</Nav.Link>\r\n                        </Nav.Item>\r\n                    </Nav>\r\n                    </Col>\r\n                    <Col sm={9}>\r\n                    <Tab.Content>\r\n                        <Tab.Pane eventKey=\"first\">\r\n                            <DetailPr\r\n                                state={props.state} brands={props.brands}\r\n                                handleChangeState={props.handleChangeState}\r\n                                type={props.type} listOfAcsrType={props.listOfAcsrType}\r\n                                getConten